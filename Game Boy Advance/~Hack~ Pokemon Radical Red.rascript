        type = collectables[cheevo]["type"]
        )
}

battleOutcome = word(0x02be8a)
inBattle = 0
win = 1

trainerBattle = bit3(0x02ab4c) == 1 && prev(bit3(0x02ab4c)) == 1
trainerID = word(0x0406ae)

function trainerWin(trainerIDs, map){
    logic = noCheats()
    logic = logic && noRando()
    logic = logic && battleOutcome == win && prev(battleOutcome) == inBattle
    logic = logic && mapID == map && prev(mapID) == map && trainerBattle
    
    logicIDs=always_false()
    for ID in trainerIDs{
        logicIDs = logicIDs || trainerID == ID
    }
    
    logic = logic && logicIDs
    return logic
}

function trainerWinRando(trainerIDs, map){
    logic = noCheats()
    logic = logic && (randomScaledSpecies == 1 || randomNormalSpecies == 1)
    logic = logic && battleOutcome == win && prev(battleOutcome) == inBattle
    logic = logic && mapID == map && prev(mapID) == map
    
    logicIDs=always_false()
    for ID in trainerIDs{
        logicIDs = logicIDs || trainerID == ID
    }
    
    logic = logic && logicIDs
    return logic
}


