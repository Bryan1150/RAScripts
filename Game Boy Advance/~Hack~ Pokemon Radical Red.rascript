function collectables(maps){
    sum = 0
    count = 0
    for item in items{
        for map in maps{
            if(map == items[item]["location"]){
                sum = sum + bit(items[item]["bit"], items[item]["address"])
                count = count+1
            }
        }
    }
    
     for item in trainers{
        for map in maps{
            if(map == trainers[item]["location"]){
                sum = sum + bit(trainers[item]["bit"], trainers[item]["address"])
                count = count+1
            }
        }
    }
    
    
    
    return measured(sum == count, when= mapID != 0 && prev(mapID) != 0  && mapID2 != 0 && prev(mapID2) != 0 && noRando() && noCheats()) && ((prev(sum) == count - 1) || prev(sum) == count - 2)
}
