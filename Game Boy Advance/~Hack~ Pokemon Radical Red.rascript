function addBits(list){
    count = 0
    sum = 0
    for item in list{
        count = count + 1
        sum = sum + item
        
    }
    return measured(sum  == count, when=mapID != 0 && prev(mapID) != 0  && mapID2 != 0 && prev(mapID2) != 0 && noRando() && noCheats()) && prev(sum) == (count -1)
}

