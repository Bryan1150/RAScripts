function calculatePokemonSum(list){
    sum = 0
    count = 0
    for pokemon in list{
        sum = sum + PokemonBit(pokemon)
        count = count + 1
    }
    return measured(sum == count, when=mapID != 0 && prev(mapID) != 0  && mapID2 != 0 && prev(mapID2) != 0 && noRando()) && prev(sum) == count -1
}

for group in specificPokemon{
