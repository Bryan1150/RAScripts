function goalList(list, type){
    logic = []
    for goal in list{
        array_push(logic, 
        once(
        InGame && MenuProgressCheck() &&
        MainMode == MainGame  && 
        (LevelID == list[goal]["id1"] ||
         LevelID == list[goal]["id2"]
        )
        && GoalType == type && prev(GoalType) == 0)
        )
    }
    return logic
}
